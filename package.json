{
    "name": "coolscript",
    "displayName": "CoolScript",
    "description": "Language support for CoolScript",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "coolscript",
            "aliases": ["CoolScript", "coolscript"],
            "extensions": [".cs",".cool",".coolscript"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "coolscript",
            "scopeName": "source.coolscript",
            "path": "./syntaxes/coolscript.tmLanguage.json"
        }]
    }
}